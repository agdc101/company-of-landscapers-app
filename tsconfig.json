{
  "compilerOptions": {
    "jsx": "react",
    "outDir": "./built",
    "allowJs": true,
    "moduleResolution": "Node16",
    "module": "Node16",
    "target": "esnext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "types": ["vite/client"]
  },
  "include": [
    "./src/**/*"
  ],
  "exclude": [
    "tests/*"
  ]
}